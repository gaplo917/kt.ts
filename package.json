{
  "name": "kt.ts",
  "version": "0.1.0",
  "description": "Porting Kotlin stdlib api to typescript",
  "main": "index.js",
  "publishConfig": {
    "access": "public"
  },
  "scripts": {
    "build": "tsc",
    "build:check": "tsc --noEmit",
    "lint-fix": "eslint 'src/**/*.{js,ts,tsx}' --quiet --fix && prettier --config .prettierrc --write 'src/**/*.{tsx,ts}'",
    "test": "jest"
  },
  "keywords": [
    "kotlin",
    "typscript"
  ],
  "author": "",
  "license": "MIT",
  "devDependencies": {
    "@babel/core": "^7.11.4",
    "@babel/preset-env": "^7.11.0",
    "@babel/preset-typescript": "^7.10.4",
    "@types/jest": "^26.0.10",
    "@types/lodash": "^4.14.159",
    "@types/node": "^14.6.0",
    "@typescript-eslint/eslint-plugin": "^3.9.1",
    "@typescript-eslint/parser": "^3.9.1",
    "babel-jest": "^26.3.0",
    "eslint": "^7.7.0",
    "eslint-config-prettier": "^6.11.0",
    "eslint-plugin-prettier": "^3.1.4",
    "jest": "^26.4.2",
    "prettier": "^2.0.5",
    "ts-node": "^9.0.0",
    "typescript": "^4.0.2"
  },
  "dependencies": {
    "lodash": "^4.17.20"
  }
}
